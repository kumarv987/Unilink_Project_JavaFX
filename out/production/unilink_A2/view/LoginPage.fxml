<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPageController">
   <children>
      <Label alignment="TOP_CENTER" contentDisplay="TOP" layoutX="-3.0" prefHeight="103.0" prefWidth="600.0" text="Welcome to the Unilink System!">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <VBox layoutX="151.0" layoutY="80.0" prefHeight="167.0" prefWidth="300.0" spacing="10.0">
         <children>
            <Label text="Enter Username:">
               <VBox.margin>
                  <Insets left="108.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="userNameTextField" maxWidth="-Infinity" prefHeight="25.0" prefWidth="120.0" promptText="Username">
               <VBox.margin>
                  <Insets left="90.0" right="50.0" />
               </VBox.margin>
               <padding>
                  <Insets left="15.0" />
               </padding>
            </TextField>
            <Button fx:id="enterButton" mnemonicParsing="false" onAction="#enterButtonPushed" text="Enter">
               <VBox.margin>
                  <Insets left="130.0" />
               </VBox.margin>
            </Button>
            <Label fx:id="userNameWarningLabel" alignment="CENTER" prefHeight="17.0" prefWidth="312.0" textAlignment="CENTER" />
         </children>
      </VBox>
      <Button fx:id="exitButton" alignment="CENTER" layoutX="544.0" layoutY="261.0" mnemonicParsing="false" onAction="#exitButtonPushed" text="EXIT" />
   </children>
</AnchorPane>
